{
    "name": "petk/php-container",
    "description": "PSR-11 compatible dependency injection container",
    "type": "package",
    "keywords": [
        "container",
        "dependency",
        "injection"
    ],
    "homepage": "https://github.com/petk/php-container",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Kokot",
            "email": "peterkokot@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Petk\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Petk\\Container\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
